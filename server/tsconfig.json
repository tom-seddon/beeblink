{
    "compilerOptions": {
        "lib": [
            "es2020"
        ],
        "module": "nodenext",
        "moduleResolution": "nodenext",
        "target": "es2020",
        "outDir": "./.build",
        "alwaysStrict": true,
        "sourceMap": true,
        "noImplicitAny": true,
        "noImplicitReturns": true,
        "noImplicitThis": true,
        "strict": true,
        "strictNullChecks": true,
        "noImplicitOverride": true,
        "strictFunctionTypes": true,
        "strictPropertyInitialization": true,
        "useUnknownInCatchVariables": true,
        "noEmitOnError": true,
        "exactOptionalPropertyTypes": true
    },
    "files": [
        "./adfsimage.ts",
        "./beebfs.ts",
        "./beeblink.ts",
        "./dfsimage.ts",
        "./dfsType.ts",
        "./diskimage.ts",
        "./errors.ts",
        "./gitattributes.ts",
        "./inf.ts",
        "./main.ts",
        "./message.ts",
        "./pcType.ts",
        "./request.ts",
        "./response.ts",
        "./server.ts",
        "./speedtest.ts",
        "./tubeHostType.ts",
        "./utils.ts",
        "./volumebrowser.ts",
        "./adfsType.ts"
    ]
}